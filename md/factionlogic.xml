	<cue name="SetupCorps" version="2">
	  <actions>
		<set_value name="md.$FactionData.{faction.otas}" exact="table[]" />
		<set_value name="md.$FactionData.{faction.jonferco}" exact="table[]" />
		<set_value name="md.$FactionData.{faction.strongarms}" exact="table[]" />
		<set_value name="md.$FactionData.{faction.nmmc}" exact="table[]" />
		<set_value name="md.$FactionData.{faction.atreus}" exact="table[]" />
		<set_value name="md.$FactionData.{faction.atf}" exact="table[]" />	
		<set_value name="md.$FactionData.{faction.xaar}" exact="table[]" />			
	  </actions>
	  <patch sinceversion="2">
		<set_value name="OTASFactionLogic.$PreferredHQSpaceMacro" exact="macro.Cluster_REM53_Sector001_macro" /><!--Legend's Home-->
		<set_value name="JonfercoFactionLogic.$PreferredHQSpaceMacro" exact="macro.Cluster_REM77_Sector001_macro" /><!--Belt of Aguilar-->
		<set_value name="StrongArmsFactionLogic.$PreferredHQSpaceMacro" exact="macro.Cluster_03_Sector001_macro" /><!--Thyn's Excavation-->
		<set_value name="NMMCFactionLogic.$PreferredHQSpaceMacro" exact="macro.Cluster_REM54_Sector001_macro" /><!--Memory of Profit IX-->
		<set_value name="AtreusFactionLogic.$PreferredHQSpaceMacro" exact="macro.Cluster_REM37_Sector001_macro" /><!--Queen's Harbour-->
		<set_value name="ATFFactionLogic.$PreferredHQSpaceMacro" exact="macro.Cluster_REM75_Sector001_macro" />	<!--Titania-->	
		<set_value name="XaarFactionLogic.$PreferredHQSpaceMacro" exact="macro.Cluster_REM50_Sector001_macro" /> <!--Emperor's Wisdom-->			
	  </patch>
	</cue>	

	<cue name="OTASFactionLogic">
	  <cues>
		<cue name="OTASFactionLogic_Manager" ref="Manager">
		  <param name="Faction" value="faction.otas"/>
		  <param name="BaseAggressionLevel" value="moodlevel.high"/>
		  <param name="BaseAvariceLevel" value="moodlevel.normal"/>
		  <param name="BaseLawfulness" value="0.6"/>
		  <param name="PreferredHQSpaceMacro" value="macro.Cluster_REM53_Sector001_macro" comment="Legend's Home"/>
		  <param name="PreferredHQTypes" value="['shipbuilding', 'equipmentdock', 'tradestation']"/>
		  <param name="SatelliteNetworkGoal" value="2"/>
		  <param name="LasertowerNetworkGoal" value="2"/>
          <param name="MinefieldGoalPerSector" value="3" comment="[MGPS * Sectors, 12].min is the maximum amount of Minefields for this faction"/>		  
		  <param name="DebugChance" value="0"/>
		  <param name="DebugChance2" value="0"/>
		</cue>
	  </cues>
	</cue>
	<cue name="JonfercoFactionLogic">
	  <cues>
		<cue name="JonfercoFactionLogic_Manager" ref="Manager">
		  <param name="Faction" value="faction.jonferco"/>
